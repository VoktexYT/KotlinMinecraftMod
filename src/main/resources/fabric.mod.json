{
  "schemaVersion": 1,
  "id": "modkotlin",
  "version": "${version}",
  "name": "modkotlin",
  "description": "",
  "authors": [],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/modkotlin/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "io.github.zalleous.modkotlin.client.ModkotlinDataGenerator"
    ],
    "client": [
      "io.github.zalleous.modkotlin.client.ModkotlinClient"
    ],
    "main": [
      { "adapter": "kotlin", "value": "io.github.zalleous.modkotlin.Modkotlin" }
    ]
  },
  "mixins": [
    "modkotlin.mixins.json",
    {
      "config": "modkotlin.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
